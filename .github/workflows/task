name: Daily cron job
on:
  schedule:
    - cron: '*/10 * * * *'
jobs:
  cron:
    runs-on: ubuntu-latest
    steps:
      - name: Daily cron jobbb
        run: |
          curl --request POST \
          --url 'https://bladeele.vercel.app/api/cron' \
          --header 'Authorization: Bearer ${{ secrets.ACTION_KEY }}'
